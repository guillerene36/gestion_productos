        </record>

        <record id="demo_producto_1" model="gestion.productos">
            <field name="name">Laptop Pro X1</field>
            <field name="tipo">bien</field>
            <field name="codigo">LP-001</field>
            <field name="origen">imp</field>
            <field name="cantidad_vendida">15</field>
            <field name="unidad_negocio_ids" eval="[(4, ref('unidad_paris')), (4, ref('unidad_easy'))]"/>
        </record>
        <record id="demo_producto_2" model="gestion.productos">
            <field name="name">Mantenimiento Preventivo</field>
            <field name="tipo">servicio</field>
