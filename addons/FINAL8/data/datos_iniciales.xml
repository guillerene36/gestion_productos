        </record>

        <record id="user_vendedor_easy" model="res.users">
            <field name="name">Vendedor Easy</field>
            <field name="login">easy@test.com</field>
            <field name="password">123</field>
            <field name="email">easy@test.com</field>
            <field name="unidad_negocio_ids" eval="[(4, ref('unidad_easy'))]"/>
            <field name="groups_id" eval="[(4, ref('base.group_user')), (4, ref('FINAL8.group_gestion_user'))]"/>
        </record>

        <record id="user_cajero_santa" model="res.users">
            <field name="name">Cajero Santa Isabel</field>
            <field name="login">santa@test.com</field>
